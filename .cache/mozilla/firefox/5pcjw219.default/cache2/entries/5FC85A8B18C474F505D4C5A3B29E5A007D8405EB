        $(document).ready(function() {
            resizeUI();
            try {
                var vUserInfo = eval('(' + CExam.AdmIndex.GetJSONUser().value + ')');
                $('#vUserName').html(vUserInfo[0].COrganName + 'ã€€' + vUserInfo[0].CUserName);
            }
            catch (ex) {
            }
            $(window).resize(function() {
                resizeUI();
            })
        });
        function resizeUI() {
            $('#outDiv').height($(window).height() - 20);
            $('#outDiv').width($(window).width() - 50);
            $('#inDiv').css('top', '50%').css('margin-top', -$('#inDiv').height() / 2).css('left', '50%').css('margin-left', -$('#inDiv').width() / 2);
        }


        function resetForm(obj) {
            obj.reset();
            createCode();
            return false;
        }

        var code; //åœ¨å…¨å±€å®šä¹‰éªŒè¯ç    
        function createCode() {
            code = "";
            var codeLength = 4; //éªŒè¯ç çš„é•¿åº¦
            var checkCode = document.getElementById("checkCode");
            var selectChar = new Array(2, 3, 4, 5, 6, 7, 8, 9, 'A', 'B', 'C', 'D', 'E', 'F', 'G', 'H', 'J', 'K', 'L', 'M', 'N', 'P', 'Q', 'R', 'S', 'T', 'U', 'V', 'W', 'X', 'Y', 'Z'); //æ‰€æœ‰å€™é€‰ç»„æˆéªŒè¯ç çš„å­—ç¬¦ï¼Œå½“ç„¶ä¹Ÿå¯ä»¥ç”¨ä¸­æ–‡çš„

            for (var i = 0; i < codeLength; i++) {
                var charIndex = Math.floor(Math.random() * 32);
                code += selectChar[charIndex];
            }

            if (checkCode)
                checkCode.value = code;
        }

        function validate() {
            var inputCode = document.getElementById("iptCode").value;
            if (inputCode.length <= 0)
                alert("è¯·è¾“å…¥éªŒè¯ç ï¼");
            else if (inputCode.toLowerCase() != code.toLowerCase()) {
                alert("éªŒè¯ç è¾“å…¥é”™è¯¯ï¼");
                createCode(); //åˆ·æ–°éªŒè¯ç 
            }
            else
                return true;

            return false;
        }
        createCode();
    </script>

    <script language="javascript" type="text/javascript">


        function Login() {
            var ipUserName = document.getElementById('ipUserName');
            var ipPassword = document.getElementById('ipPassword');
            if (trim(ipUserName.value) == '') {
                divInfo.innerText = 'ç”¨æˆ·åä¸ºç©ºï¼';
                return;
            }

            if (trim(ipPassword.value) == '') {
                divInfo.innerText = 'å¯†ç ä¸ºç©ºï¼';
                return;
            }

            if (!validate())
                return;

            setTimeout(function() {
                try {
                    if (CExam.Login.UserLogin(ipUserName.value, ipPassword.value).value) {
                        divInfo.innerHTML = 'ç™»é™†æˆåŠŸï¼Œä¸€ç§’åè‡ªåŠ¨è·³è½¬';
                        setTimeout(function() {
                            window.parent.location = 'default.aspx';
                        }, 1000);
                    }
                    else
                        divInfo.innerHTML = 'èº«ä»½ä¸åˆæ³•';
                }
                catch (ex) {
                    divInfo.innerText = 'æœåŠ¡å™¨é€šä¿¡å¤±è´¥ï¼';
                }
            }, 1);
        }
    </script>

